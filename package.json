    "watch": "npm run build -- -w",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "concat-stream": "1.6.0",
    "is-stream": "1.1.0",
    "through2": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.0.2",
    "mocha": "3.4.2",
    "npm-run-all": "4.0.2",
    "snazzy": "7.0.0",
    "standard": "10.0.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator",
