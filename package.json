{
  "name": "checksum-node",
  "version": "1.0.0",
  "description": "A checksum for Node",
  "author": {
    "name": "RÃ´mulo Cabral Santos",
    "url": "http://romulosanttos.github.io",
    "email": "romulosanttos1@gmail.com"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "stream",
    "checksum",
    "sum",
    "shasum",
    "hash",
    "sha1",
    "sha256",
    "md5",
    "snapshot",
    "filesize"
  ],
  "files": [
    "dist/",
    "license",
    "readme.md"
  ],
  "main": "dist/index.js",
  "scripts": {
